{
    "name": "jolitagrazyte/discogs-api",
    "description": "A simple Discogs Api wrapper using Guzzle7.",
    "keywords": [
        "Discogs Api"
    ],
    "homepage": "https://github.com/jolitagrazyte/discogs-api-wrapper",
    "license": "MIT",
    "authors": [
        {
            "name": "Jolita Grazyte",
            "email": "hello@jolitagraze.com",
            "homepage": "http://jolitagraze.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.4|^8.1|^8.2",
        "illuminate/support": "^9.28|^10.0",
        "guzzlehttp/guzzle": "^7.2",
        "ext-json": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^9.5|^10.0",
        "mockery/mockery": "^1.4.4"
    },
    "autoload": {
        "psr-4": {
            "Jolita\\DiscogsApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jolita\\DiscogsApi\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
